{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "datetime": {
      "type": "datetime"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "instructor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::instructor.instructor",
      "inversedBy": "events"
    },
    "event_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event-category.event-category",
      "inversedBy": "events"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "studio": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::studio.studio",
      "inversedBy": "events"
    },
    "spots": {
      "type": "integer"
    },
    "minutes": {
      "type": "integer"
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "event"
    },
    "recurrence_interval": {
      "type": "integer"
    },
    "recurrence_end_date": {
      "type": "date"
    },
    "recurrence_time": {
      "type": "time"
    },
    "recurrence_weekday": {
      "type": "enumeration",
      "enum": [
        "monday",
        "tuesday",
        "wednesday",
        "thursday",
        "friday",
        "saturday",
        "sunday"
      ]
    }
  }
}
